{
  "name": "poddr",
  "productName": "Poddr",
  "version": "2.1.0",
  "description": "Podcast client",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sn8z/Poddr.git"
  },
  "private": true,
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "zone.js"
    ],
    "files": [
      "tests/*.spec.ts",
      "src/app/*/*.spec.ts"
    ],
    "verbose": true,
    "concurrency": 1,
    "workerThreads": false
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration production",
    "electron": "./node_modules/.bin/electron .",
    "start": "yarn build && ./node_modules/.bin/electron .",
    "start:debug": "yarn build && ./node_modules/.bin/electron . --debug",
    "dist": "yarn build && electron-builder",
    "dist:mac": "yarn build && electron-builder --mac",
    "dist:win": "yarn build && electron-builder --win",
    "dist:linux": "yarn build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "lint": "ng lint",
    "test": "ava"
  },
  "author": "Marcus Sneitz <marcus.sneitz@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/build": "^19.1.1",
    "@angular/cli": "19.1.1",
    "@angular/compiler-cli": "19.1.1",
    "@angular/language-service": "^19.1.1",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.18.0",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3",
    "worker-plugin": "^5.0.1"
  },
  "dependencies": {
    "@angular/animations": "19.1.1",
    "@angular/common": "19.1.1",
    "@angular/compiler": "19.1.1",
    "@angular/core": "19.1.1",
    "@angular/forms": "19.1.1",
    "@angular/platform-browser": "19.1.1",
    "@angular/platform-browser-dynamic": "19.1.1",
    "@angular/router": "19.1.1",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-select/ng-select": "^14.2.0",
    "@simonwep/pickr": "^1.8.0",
    "@sn8z/pop-pop.css": "^1.0.2",
    "dbus-next": "^0.10.2",
    "electron-log": "^5.2.4",
    "electron-store": "8.2.0",
    "electron-window-state": "^5.0.3",
    "mousetrap": "^1.6.3",
    "mpris-service": "2.1.2",
    "node-podcast-parser": "^2.0.0",
    "rxjs": "~7.8.1",
    "semver": "^7.3.7",
    "sweetalert2": "^11.7.20",
    "tslib": "^2.4.0",
    "typeface-nunito-sans": "^1.1.13",
    "typeface-open-sans": "^1.1.13",
    "zone.js": "~0.15.0"
  }
}
