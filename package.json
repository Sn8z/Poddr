{
  "name": "poddr",
  "productName": "Poddr",
  "version": "2.1.0",
  "description": "Podcast client",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sn8z/Poddr.git"
  },
  "private": true,
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "zone.js"
    ],
    "files": [
      "tests/*.spec.ts",
      "src/app/*/*.spec.ts"
    ],
    "verbose": true,
    "concurrency": 1,
    "workerThreads": false
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --buildOptimizer=true --configuration production",
    "electron": "./node_modules/.bin/electron .",
    "start": "yarn build && ./node_modules/.bin/electron .",
    "start:debug": "yarn build && ./node_modules/.bin/electron . --debug",
    "dist": "yarn build && electron-builder",
    "dist:mac": "yarn build && electron-builder --mac",
    "dist:win": "yarn build && electron-builder --win",
    "dist:linux": "yarn build && electron-builder --linux",
    "postinstall": "ngcc && electron-builder install-app-deps",
    "lint": "ng lint",
    "test": "ava"
  },
  "author": "Marcus Sneitz <marcus.sneitz@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "13.3.9",
    "@angular/language-service": "^13.3.9",
    "@types/node": "17.0.35",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "ava": "4.0.1",
    "electron": "^19.0.9",
    "electron-builder": "^22.10.5",
    "eslint": "^7.26.0",
    "spectron": "^14.0.0",
    "ts-node": "10.8.0",
    "typescript": "^4.6.4",
    "worker-plugin": "^5.0.1"
  },
  "dependencies": {
    "@angular/animations": "13.3.9",
    "@angular/common": "13.3.9",
    "@angular/compiler": "13.3.9",
    "@angular/core": "13.3.9",
    "@angular/forms": "13.3.9",
    "@angular/platform-browser": "13.3.9",
    "@angular/platform-browser-dynamic": "13.3.9",
    "@angular/router": "13.3.9",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-select/ng-select": "8.1.1",
    "@simonwep/pickr": "^1.8.0",
    "@sn8z/pop-pop.css": "^1.0.2",
    "dbus-next": "^0.10.2",
    "electron-log": "4.4.7",
    "electron-store": "^8.0.1",
    "electron-window-state": "^5.0.3",
    "mousetrap": "^1.6.3",
    "mpris-service": "2.1.2",
    "node-podcast-parser": "^2.0.0",
    "rxjs": "~7.5.3",
    "semver": "^7.3.7",
    "sweetalert2": "^11.4.16",
    "tslib": "^2.4.0",
    "typeface-nunito-sans": "^1.1.13",
    "typeface-open-sans": "^1.1.13",
    "zone.js": "~0.11.4"
  }
}
