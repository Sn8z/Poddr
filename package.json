{
  "name": "poddr",
  "productName": "Poddr",
  "version": "2.1.0",
  "description": "Podcast client",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sn8z/Poddr.git"
  },
  "private": true,
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "zone.js"
    ],
    "files": [
      "tests/*.spec.ts",
      "src/app/*/*.spec.ts"
    ],
    "verbose": true,
    "concurrency": 1,
    "workerThreads": false
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration production",
    "electron": "./node_modules/.bin/electron .",
    "start": "yarn build && ./node_modules/.bin/electron .",
    "start:debug": "yarn build && ./node_modules/.bin/electron . --debug",
    "dist": "yarn build && electron-builder",
    "dist:mac": "yarn build && electron-builder --mac",
    "dist:win": "yarn build && electron-builder --win",
    "dist:linux": "yarn build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "lint": "ng lint",
    "test": "ava"
  },
  "author": "Marcus Sneitz <marcus.sneitz@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@angular-builders/custom-webpack": "18.0.0",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/build": "^18.2.12",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.0.5",
    "@angular/language-service": "^18.0.5",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "eslint": "^8.57.0",
    "ts-node": "10.9.1",
    "typescript": "5.4.5",
    "worker-plugin": "^5.0.1"
  },
  "dependencies": {
    "@angular/animations": "18.0.5",
    "@angular/common": "18.0.5",
    "@angular/compiler": "18.0.5",
    "@angular/core": "18.0.5",
    "@angular/forms": "18.0.5",
    "@angular/platform-browser": "18.0.5",
    "@angular/platform-browser-dynamic": "18.0.5",
    "@angular/router": "18.0.5",
    "@fortawesome/angular-fontawesome": "0.15.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-select/ng-select": "^13.3.0",
    "@simonwep/pickr": "^1.8.0",
    "@sn8z/pop-pop.css": "^1.0.2",
    "dbus-next": "^0.10.2",
    "electron-log": "4.4.8",
    "electron-store": "^8.0.1",
    "electron-window-state": "^5.0.3",
    "mousetrap": "^1.6.3",
    "mpris-service": "2.1.2",
    "node-podcast-parser": "^2.0.0",
    "rxjs": "~7.8.1",
    "semver": "^7.3.7",
    "sweetalert2": "^11.7.20",
    "tslib": "^2.4.0",
    "typeface-nunito-sans": "^1.1.13",
    "typeface-open-sans": "^1.1.13",
    "zone.js": "~0.14.7"
  }
}